@model IEnumerable<CrudTest.Entities.Category>

<div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
    <ul class="navbar-nav flex-grow-1">
        @if (Model != null)
        {
            foreach (var category in Model)
            {
                <li class="nav-item">
                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Category" asp-route-name="@category.Name">@category.Name</a>
                </li>
            }
        }
    </ul>
</div>
